<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- 
  - This file is included by the struts.xml file as an example
  - of how to break up the configuration file into multiple files.
-->
<struts>



    <package name="product" namespace="/product" extends="iolt-default">


	<!-- <default-interceptor-ref name="loggedinuser" /> -->
	
	
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult"/>
        </result-types>


        
         <action name="index" class="sg.com.ioutlet.web.app.product.action.ProductIndexAction" method="input">
            <result name="input" type="tiles">SG.COM.IOUTLET.PRODUCT.INDEX</result>
        </action>


     <action name="form!*" class="sg.com.ioutlet.web.app.product.action.ProductIndexAction" method="{*}">
            <result name="input" type="tiles">SG.COM.IOUTLET.USER.REG</result>
            <result name="success" type="tiles">SG.COM.IOUTLET.USER.SUCCESS</result>
    
    		<!-- DISPLAY PAGE -->
			<result name="back" type="redirect-action">
				<param name="namespace">
					/
				</param>
				<param name="actionName">register!input</param>				
			</result>
			
			
        </action>
        
        
        <!-- Add additional "example" package actions here. -->

    </package>

</struts>