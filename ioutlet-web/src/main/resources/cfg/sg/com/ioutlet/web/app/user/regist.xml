<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- 
  - This file is included by the struts.xml file as an example
  - of how to break up the configuration file into multiple files.
-->



<struts>



    <package name="user" namespace="/regist" extends="iolt-default">


        
         <action name="index" class="sg.com.ioutlet.web.app.user.action.UserRegistIndexAction" method="input">
            <result name="input" type="tiles">SG.COM.IOUTLET.USER.REG</result>
            <result name="success" type="tiles">SG.COM.IOUTLET.DASH.INDEX</result>
    
        </action>
        
        
         
            <action name="jsonAct" class="sg.com.ioutlet.web.app.user.action.UserRegistIndexAction" method="json">
                <result type="json">
                	<param name="root">model</param>
                	 <param name="ignoreHierarchy">false</param>  
                </result>
            </action>
        
        
        
        
        <action name="form!*" class="sg.com.ioutlet.web.app.user.action.UserRegistIndexAction" method="{*}">
            <result name="input" type="tiles">SG.COM.IOUTLET.USER.REG</result>
          
    		<!-- DISPLAY PAGE -->
			<result name="success" type="redirect-action">
				<param name="namespace">/login</param>
				<param name="actionName">index!input</param>				
			</result>
			
			
        </action>
        

        <!-- Add additional "example" package actions here. -->

    </package>

</struts>