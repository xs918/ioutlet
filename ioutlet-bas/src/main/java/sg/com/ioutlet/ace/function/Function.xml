<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">
	<named-query name="Function.getAll">
		<query>
			SELECT f
			FROM Function f
			WHERE f.recordStatus='A'
		</query>
	</named-query>




	<entity class="sg.com.ioutlet.ace.function.Function" access="FIELD">
		<table name="FUNCTIONS" />



		<attributes>
			<embedded-id name="key" />
			
			
			<basic name="id" optional="false">
				<column name="ID" nullable="false" length="50" />
				<enumerated>STRING</enumerated>
			</basic>
			
			
		
			<basic name="name" optional="false">
				<column name="NAME" nullable="false" length="50" />
			</basic>

			<basic name="description" optional="true">
				<column name="DESCRIPTION" nullable="true" length="255" />
			</basic>
			
				<basic name="action" optional="true">
				<column name="ACTION" nullable="true" length="255" />
			</basic>
			
			<basic name="namespace" optional="true">
				<column name="NAME_SPACE" nullable="true" length="255" />
			</basic>
			
	

			<many-to-one name="domain" optional="false">
				<join-column name="DOMAIN_UUID" referenced-column-name="UUID"
					insertable="true" updatable="true" />
			</many-to-one>
			
		    <one-to-many name="accessFunctions" mapped-by="function">
		    <cascade>
					<cascade-merge />
					<cascade-remove />
				</cascade>
		   </one-to-many>
			
			



		</attributes>
	</entity>
	<embeddable class="sg.com.ioutlet.ace.function.FunctionKey"
		access="FIELD">
		<attributes>
			<basic name="uuid" optional="false">
				<column name="UUID" nullable="false" length="40" />
			</basic>
		</attributes>
	</embeddable>

</entity-mappings>

 