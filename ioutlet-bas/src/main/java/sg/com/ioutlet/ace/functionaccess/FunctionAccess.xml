<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">
	<named-query name="FunctionAccess.getByRoleUuid.String">
		<query>
			SELECT u 
			FROM User u
			WHERE u.role.key.uuid = :ROLE_UUID
			AND u.recordStatus='A'
		</query>
	</named-query>


	<named-query name="FunctionAccess.getAll">
		<query>
			SELECT u
			FROM User u
			WHERE u.recordStatus='A'
		</query>
	</named-query>




	<entity class="sg.com.ioutlet.ace.functionaccess.FunctionAccess" access="FIELD">
		<table name="FUNCTION_ACCESS" />



		<attributes>
			<embedded-id name="key" />
			<basic name="maxLimit" optional="true">
				<column name="MAX_LIMIT" nullable="true"/>
			</basic>

			<many-to-one name="role" optional="true">
				<join-column name="ROLE_UUID" referenced-column-name="UUID"
					insertable="true" updatable="true" />
			</many-to-one>
			
			
			
			 <many-to-one name="function" optional="true">
				<join-column name="FUNCTION_UUID" referenced-column-name="UUID"
					insertable="true" updatable="true" />
			</many-to-one>
			
			
		
			



		</attributes>
	</entity>
	<embeddable class="sg.com.ioutlet.ace.functionaccess.FunctionAccessKey"
		access="FIELD">
		<attributes>
			<basic name="uuid" optional="false">
				<column name="UUID" nullable="false" length="40" />
			</basic>
		</attributes>
	</embeddable>

</entity-mappings>

 