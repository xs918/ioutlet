<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">
	
  
  
   	<named-query name="BizInfo.getAll">
		<query>
			SELECT s 
			FROM BizInfo s
			where s.recordStatus='A'
		</query>
	</named-query> 
	
	
	



    <entity class="sg.com.ioutlet.model.bizinfo.BizInfo"
          access="FIELD">
    <table name="BIZ_INFOS"/>
    
    
      
      <attributes>
       	<embedded-id name="key" />
       	
       	 <basic name="regId" optional="false">
			<column name="REG_ID" nullable="false" length="50"/>
	    </basic>
	    
	    
       
      	<basic name="regName" optional="false">
			<column name="REG_NAME" nullable="false" length="50"/>
	    </basic>
	
	
	
		<basic name="telPhone" optional="true">
			<column name="TEL_PHONE" nullable="true" length="50"/>
	    </basic>
	
	
		
	    <basic name="addr1" optional="false">
			<column name="ADDRESS1" nullable="false" length="150"/>
	    </basic>
	        <basic name="addr2" optional="false">
			<column name="ADDRESS2" nullable="false" length="150"/>
	    </basic>
	        <basic name="addr3" optional="true">
			<column name="ADDRESS3" nullable="true" length="150"/>
	    </basic>
	
	    
	    <basic name="postCode" optional="false">
			<column name="POST_CODE" nullable="false" length="200"/>
	    </basic>
	  	
		
			<one-to-one name="user" optional="true">
				<join-column name="USER_UUID" referenced-column-name="UUID" />
			</one-to-one>
		
	    
	    </attributes>
	 </entity>
	<embeddable class="sg.com.ioutlet.model.bizinfo.BizInfoKey"
		access="FIELD">
		<attributes>
			<basic name="uuid" optional="false">
				<column name="UUID" nullable="false" length="40" />
			</basic>
		</attributes>
	</embeddable>
	
	
 </entity-mappings>

 